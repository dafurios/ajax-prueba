<%= form_with(model: [@enterprise], html: { class: 'form-horizontal' }) do |f| %>

<tr id="<%= @enterprise.id %>">
  <td><%= @enterprise.name %></td>
  <td><%= link_to 'Show', @enterprise %></td>
  <td><%= link_to 'Edit', edit_enterprise_path(@enterprise), data: {remote: true} %></td>
  <td><%= link_to 'Destroy', @enterprise, method: :delete, data: { confirm: 'Are you sure?' , remote: true} %></td>
</tr>
<% end %>
